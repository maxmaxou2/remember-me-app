{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "lib": [
            "esnext"
        ],
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "allowJs": true,
        "skipLibCheck": true,
        "noEmit": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "strict": true,
        "baseUrl": "./src",
        "paths": {
            "@/*": [
                "./*"
            ]
        }
    }
}
